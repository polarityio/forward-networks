  "block": {
    "component": {
      "file": "./client/block.js"
    },
    "template": {
      "file": "./client/block.hbs"
    }
  },
  "request": {
    "cert": "",
    "key": "",
    "passphrase": "",
    "ca": "",
    "proxy": ""
  },
  "logging": {
    "level": "info"
  },
  "options": [
    {
      "key": "url",
      "name": "Dataminr API URL",
